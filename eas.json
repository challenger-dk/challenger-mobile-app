{
  "cli": {
    "version": ">= 13.2.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL}",
        "EXPO_PUBLIC_TOMTOM_API_KEY": "${EXPO_PUBLIC_TOMTOM_API_KEY}",
        "EXPO_PUBLIC_CHAT_API_URL": "${EXPO_PUBLIC_CHAT_API_URL}",
        "EXPO_PUBLIC_WS_URL": "${EXPO_PUBLIC_WS_URL}",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "${EXPO_PUBLIC_GOOGLE_MAPS_API_KEY}"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL}",
        "EXPO_PUBLIC_TOMTOM_API_KEY": "${EXPO_PUBLIC_TOMTOM_API_KEY}",
        "EXPO_PUBLIC_CHAT_API_URL": "${EXPO_PUBLIC_CHAT_API_URL}",
        "EXPO_PUBLIC_WS_URL": "${EXPO_PUBLIC_WS_URL}",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "${EXPO_PUBLIC_GOOGLE_MAPS_API_KEY}"
      }
    },
    "production": {
      "ios": {
        "autoIncrement": true
      },
      "android": {
        "autoIncrement": true
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "${EXPO_PUBLIC_API_BASE_URL}",
        "EXPO_PUBLIC_TOMTOM_API_KEY": "${EXPO_PUBLIC_TOMTOM_API_KEY}",
        "EXPO_PUBLIC_CHAT_API_URL": "${EXPO_PUBLIC_CHAT_API_URL}",
        "EXPO_PUBLIC_WS_URL": "${EXPO_PUBLIC_WS_URL}",
        "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY": "${EXPO_PUBLIC_GOOGLE_MAPS_API_KEY}"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal"
      }
    }
  }
}
