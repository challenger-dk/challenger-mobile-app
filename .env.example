# Main API URL (default: http://localhost:3000 for iOS, http://10.0.2.2:3000 for Android)
EXPO_PUBLIC_API_BASE_URL=http://localhost:3000

# Chat Service URL (default: http://localhost:8002 for iOS, http://10.0.2.2:8002 for Android)
# This is the conversation-based chat API
EXPO_PUBLIC_CHAT_API_URL=http://localhost:8002

# WebSocket URL (default: ws://localhost:8002 for iOS, ws://10.0.2.2:8002 for Android)
# Used for real-time messaging
EXPO_PUBLIC_WS_URL=ws://localhost:8002

# TomTom API Key for maps
EXPO_PUBLIC_TOMTOM_API_KEY=your-tomtom-api-key

# Google Maps API Key
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=
